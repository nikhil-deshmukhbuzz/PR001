<div class="modal fade" id="mediumModal" tabindex="-1" aria-labelledby="staticModalLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="mediumModalLabel">License</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" ng-click="reset(form)">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form name="form" novalidate>
                <div class="modal-body">
                    <div>
                        <div class="card">
                            <div class="card-header"><strong>{{header}}</strong></div>
                            <div class="card-body card-block">
                                <div class="row">
                                    <div class="col">
                                        <div class="form-group">
                                            <label for="client" class=" form-control-label">Client<span class="error">*</span></label>

                                            <select id="client" name="client" data-placeholder="Choose a Client..." class="standardSelect form-control" tabindex="1" ng-class="{'has-error': form.client.$error.required}" ng-model="ddl.selectedClient" ng-required="true" ng-disabled="isEditable"></select>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="form-group" style="font-size: 12px;padding-top: 35px;" ng-show="IsLicenseExists">
                                            License of all product already added.
                                        </div>
                                    </div>
                                </div>
                                <div class="row" ng-repeat="item in listOfProduct">
                                    <div class="col">
                                        <div class="form-group">
                                            <label class=" form-control-label">Product Name</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="fa fa-windows"></i></div>
                                                <input class="form-control" name="productName" ng-model="item.ProductName" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="form-group">
                                            <label class=" form-control-label">Total License</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="fa fa-certificate"></i></div>
                                                <input class="form-control" name="license{{$index}}" ng-model="item.TotalLicense" only-digits maxlength="3" ng-class="{ 'has-error' : form.license.$invalid && !form.license.$pristine }" autocomplete="off" required>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="form-group">
                                            <label class=" form-control-label">License Due Date</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="fa fa-calendar"></i></div>
                                                <input type="date" class="form-control" name="date" ng-model="item.LicenseDueDate" autocomplete="off">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button id="close" type="button" class="btn btn-secondary" data-dismiss="modal" ng-click="reset(form)">Cancel</button>
                    @*<button id="delete" type="button" class="btn btn-danger" data-dismiss="modal" ng-click="delete(form)" ng-show="isEditable && !isRegister">Delete</button>*@
                    <button type="submit" ng-click="submit(form)" ng-disabled="form.client.$pristine || form.$pristine || IsLicenseExists" class="btn btn-primary" data-dismiss="modal">{{submitTxt}}</button>
                </div>
            </form>
        </div>
    </div>
</div>
